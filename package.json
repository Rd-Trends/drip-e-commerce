{
  "name": "drip-e-commerce",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "build:ci": "pnpm generate:importmap && payload migrate && pnpm build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "db:seed": "cross-env NODE_OPTIONS=--no-deprecation payload run src/endpoints/seed/index.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-postgres": "3.68.5",
    "@payloadcms/email-nodemailer": "3.68.5",
    "@payloadcms/live-preview-react": "3.68.5",
    "@payloadcms/next": "3.68.5",
    "@payloadcms/plugin-ecommerce": "3.68.5",
    "@payloadcms/plugin-form-builder": "3.68.5",
    "@payloadcms/plugin-seo": "3.68.5",
    "@payloadcms/richtext-lexical": "3.68.5",
    "@payloadcms/storage-s3": "3.68.5",
    "@payloadcms/ui": "3.68.5",
    "@paystack/inline-js": "^2.22.7",
    "@paystack/paystack-sdk": "^1.0.1",
    "@react-email/components": "1.0.2",
    "@tanstack/react-query": "^5.90.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.12.0",
    "lucide-react": "^0.559.0",
    "next": "15.4.10",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.5",
    "payload": "3.68.5",
    "postcss": "^8.5.6",
    "qs-esm": "^7.0.2",
    "query-string": "^9.3.1",
    "react": "19.2.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.1",
    "react-email": "^5.1.0",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.6.0",
    "shadcn": "^3.6.2",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.19.3",
    "@types/paystack__inline-js": "^1.0.2",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
